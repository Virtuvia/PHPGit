{
    "name": "kzykhys/git",
    "description": "A Git wrapper for PHP5.3+",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Kazuyuki Hayashi",
            "email": "hayashi@valnur.net"
        }
    ],
    "require": {
        "php": "^5.3.2 || ^7.3",
        "symfony/process": "^3.4",
        "symfony/options-resolver": "^3.4"
    },
    "require-dev": {
        "symfony/filesystem": "^3.4"
    },
    "autoload": {
        "psr-0": {
            "": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}